    <script>
      function incerease() {
        let value = document.getElementById("amtv").innerText;
        console.log("HELLO");
        document.getElementById("amtv").innerText++;
      }

      function decrease() {
        let value = document.getElementById("amtv").innerText;
        if (value > 1) {
          document.getElementById("amtv").innerText--;
        }
