    <script>

      function pay() {
        console.log(document.getElementById("monthno").innerHTML);
        document.getElementById("pay").click();
      }
      function option1() {
        console.log("clicked1");
        let price1 = document.getElementById("price1");
        let price2 = document.getElementById("price2");
        price2.style.border = "1px solid gray";
        price1.style.border = "4px solid darkgreen";

        document.getElementById("total").value = document.getElementById("monthval").innerHTML;
        document.getElementById("mno").value = document.getElementById("monthno").innerHTML;
        document.getElementById("num").value = document.getElementById("monthno").innerHTML;

        document.getElementById("lesscheck").checked = true;
        document.getElementById("lesscheck").required = true;
        document.getElementById("largecheck").required = false;
      }
      function option2() {
        console.log("clicked2");
        let price1 = document.getElementById("price1");
        let price2 = document.getElementById("price2");
        price1.style.border = "1px solid gray";
        price2.style.border = "4px solid darkgreen";

        document.getElementById("total").value = 1, 799.99;
        document.getElementById("mno").value = 12;

        document.getElementById("largecheck").checked = true;
        document.getElementById("largecheck").required = true;
        document.getElementById("lesscheck").required = false;
      }


      function incre() {
        let value = document.getElementById("monthno").innerText;
        console.log(value);
        if (value < 12) {
          document.getElementById("monthval").innerText = parseInt(150, 10) + parseInt(document.getElementById("monthval").innerText, 10);
          document.getElementById("monthno").innerText++;
          document.getElementById("pricemonth").innerText++;
        }
      }

      function decre() {
        let value = document.getElementById("monthno").innerText;
        if (value > 4) {
          document.getElementById("monthval").innerText -= 150;
          document.getElementById("monthno").innerText--;
          document.getElementById("pricemonth").innerText--;
        }
      }
    </script>



