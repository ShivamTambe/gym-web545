      <div class="pricingt">Purchase a minimum of 4 Months
        <!-- <span id="pricemonth" id="monthval">4</span> -->
      </div>
      <div class="txt">
        <div class="monthprice mp">
          <div class="pricee" id="price1" onclick="option1()">
            <input type="checkbox" name="less" id="lesscheck" required class="che">
            <div class="signn">$</div>
            <div class="valuee"><span id="monthval">599</span>.99</div>
            <div class="monthh"><span id="monthno">4</span> /months</div>
            <div class="arr">
              <div class="uparr"><img src="/imgs/right-arrow.png" onclick="incre()" alt=""></div>
              <div class="downarr"><img src="/imgs/right-arrow.png" onclick="decre()" alt=""></div>
            </div>
          </div>
          <div class="pricee" id="price2" onclick="option2()">
            <input type="checkbox" name="large" id="largecheck" required class="che">
            <div class="signn">$</div>
            <div class="valuee"><span class="yearval">1,799.99</span></div>
            <div class="monthh"><span id="no"></span> /Yearly</div>
          </div>
          <div class="taxtitle">8.25% sales tax</div>
          <div class="taxtitle">8.25% sales tax</div>
        </div>
        <input type="text" id="total" name="total">
        <input type="text" name="mno" id="mno">
        <div class="btmtxt">Pay for the entire year and get 16.6% off the total price</div>
      </div>
      <div class="pricingdis">
        <div class="pricingbtns">
          <div class="btns">
            <button class="btn btn2 signbtna" onclick="pay()">Sign Me Up</button>
            <button type="submit" id="pay"></button>
          </div>
        </div>
