<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="/style.css">
    <title>GYms</title>
</head>
<body>
    <%- include('../partials/header.ejs'); %>

    <div class="allgyms">
        <% for(var i=0; i<item.length; i++) {%>
            <div class="gyminfo">
                <div class="profileimg"><img src="/imgs/gymlogo1.png" alt="" class="img"></div>
                <div class="gymname"><%= item[i].gymname%></div>
                <div class="gymname"><%= item[i].name%></div>
                <div class="otherinfo">
                    <div class="gymbio"><%= item[i].gymbio %></div>
                    <div class="state"><%= item[i].city %>,<%= item[i].state %></div>
                    <div class="city"> </div>
                </div>
            </div>
         <% } %>
    </div>




    <%- include('../partials/fotter.ejs'); %>
</body>
</html>